{
      "trailingComma": "all",
      "tabWidth": 6,
      "printWidth": 150,
      "semi": true,
      "singleQuote": true,
      "useTabs": false,
      "quoteProps": "as-needed",
      "jsxSingleQuote": false,
      "bracketSpacing": true,
      "jsxBracketSameLine": true,
      "arrowParens": "avoid",
      "requirePragma": false,
      "insertPragma": false,
      "proseWrap": "preserve",
      "endOfLine": "lf"
}
