{
    "trailingComma": "all",
    "tabWidth": 4,
    "printWidth": 200,
    "semi": true,
    "singleQuote": false,
    "useTabs": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "endOfLine": "lf"
}
